{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "declarationDir": "./dist/typings",
        "target": "es6",
        "outDir": "./dist",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "pretty": true,
        "removeComments": true,
        "moduleResolution": "node",
        "suppressImplicitAnyIndexErrors": true
    },
    "exclude": [
        "node_modules",
        "test",
        "dist"
    ]
}